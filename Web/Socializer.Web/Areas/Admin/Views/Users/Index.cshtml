@model Socializer.Web.ViewModels.Users.ProfileViewModel

<div class="row text-center">
    <div class="col">
        <h3>Profile</h3>
        <h4>@Model.Id</h4>
        <h5>@Model.UserName</h5>
        @foreach (var post in Model.Posts)
        {
            <partial model="post" name="_PostPartial"/>
        }
    </div>
    <div class="col">
        <h3>Settings</h3>
    </div>
</div>